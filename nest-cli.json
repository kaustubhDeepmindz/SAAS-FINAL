{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth-services/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth-services",
  "projects": {
    "auth-services": {
      "type": "application",
      "root": "apps/auth-services",
      "entryFile": "main",
      "sourceRoot": "apps/auth-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-services/tsconfig.app.json"
      }
    },
    "payment-services": {
      "type": "application",
      "root": "apps/payment-services",
      "entryFile": "main",
      "sourceRoot": "apps/payment-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-services/tsconfig.app.json"
      }
    },
    "billing-services": {
      "type": "application",
      "root": "apps/billing-services",
      "entryFile": "main",
      "sourceRoot": "apps/billing-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing-services/tsconfig.app.json"
      }
    },
    "manager-services": {
      "type": "application",
      "root": "apps/manager-services",
      "entryFile": "main",
      "sourceRoot": "apps/manager-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/manager-services/tsconfig.app.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "repository": {
      "type": "library",
      "root": "libs/repository",
      "entryFile": "index",
      "sourceRoot": "libs/repository/src",
      "compilerOptions": {
        "tsConfigPath": "libs/repository/tsconfig.lib.json"
      }
    }
  }
}